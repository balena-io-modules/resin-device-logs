{
  "name": "resin-device-logs",
  "version": "3.2.1",
  "description": "Resin.io device logs utilities",
  "main": "build/logs.js",
  "homepage": "https://github.com/resin-io/resin-device-logs",
  "repository": {
    "type": "git",
    "url": "git://github.com/resin-io/resin-device-logs.git"
  },
  "keywords": [
    "resin",
    "logs",
    "device"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "npm run test-node && npm run test-browser",
    "test-node": "gulp test",
    "test-browser": "karma start",
    "lint": "gulp lint",
    "build": "gulp build",
    "prepublish": "require-npm4-to-publish",
    "prepublishOnly": "npm test && npm run build",
    "readme": "jsdoc2md --template doc/README.hbs build/logs.js > README.md"
  },
  "author": "Juan Cruz Viotti <juan@resin.io>",
  "license": "Apache-2.0",
  "devDependencies": {
    "coffee-script": "~1.12.0",
    "gulp": "^3.8.10",
    "gulp-coffee": "^2.2.0",
    "gulp-coffeelint": "^0.6.0",
    "gulp-mocha": "^4.0.0",
    "gulp-util": "^3.0.1",
    "jsdoc-to-markdown": "^3.0.0",
    "karma": "^1.3.0",
    "mocha": "^3.0.0",
    "mochainon": "^1.0.0",
    "resin-config-karma": "^1.0.4",
    "require-npm4-to-publish": "^1.0.0"
  },
  "dependencies": {
    "bluebird": "^3.0.0",
    "lodash": "^4.0.0",
    "pubnub": "~4.15.0"
  }
}